{
  "name": "aivec/welcart-settlement-modules",
  "description": "Code for creating settlement modules and accompanying hooks/filters given a set of configuration parameters",
  "type": "library",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Evan Shaw",
      "email": "evandanielshaw@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://bitbucket.org/aivec/aauth"
    }
  ],
  "require": {
    "aivec/welcart-generic": ">=2.0 <4.0",
    "aivec/aauth": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Aivec\\Welcart\\SettlementModules\\": ""
    }
  },
  "require-dev": {
    "aivec/phpcs-wp": "^1.0"
  },
  "scripts": {
    "lint": [
      "phpcs --standard=Aivec --extensions=php ."
    ],
    "lint-fix": [
      "phpcbf --standard=Aivec --extensions=php ."
    ]
  }
}
